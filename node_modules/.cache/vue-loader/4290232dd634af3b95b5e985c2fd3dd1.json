.post.Image,\n\n            };\n\n            PostDataService.create(data)\n            .then(response => {\n                this.post.id = response.data.id;\n                console.log(response.data);\n                this.submitted = true;\n            })\n            .catch(e => {\n                console.log(e);\n            });\n        },\n\n        newPost() {\n            this.submitted = false;\n            this.post = {};\n        }\n    }\n};\n</script>\n\n<style>\n.submit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>"]}]}